# API Configuration
API_VERSION=v1
DEBUG=true

# Stock Data Configuration
DEFAULT_SCAN_LIMIT=100
MAX_CONCURRENT_REQUESTS=10

# Rate Limiting
API_RATE_LIMIT=100

# Logging
LOG_LEVEL=INFO