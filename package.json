{
  "name": "stockbreak-pro",
  "version": "2.0.0",
  "description": "A professional-grade Indian stock market breakout screener",
  "scripts": {
    "install-all": "cd frontend && npm install && cd ../backend && pip install -r requirements.txt",
    "start-frontend": "cd frontend && npm start",
    "start-backend": "cd backend && python -m uvicorn server:app --host 0.0.0.0 --port 8001 --reload",
    "test-frontend": "cd frontend && npm test",
    "test-backend": "cd backend && python -m pytest tests/ -v"
  },
  "keywords": ["stock", "trading", "breakout", "screener", "nse", "india"],
  "author": "StockBreak Pro Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/stockbreak-pro.git"
  }
}